<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="org.beautiful_butterflies.quirky_quarks.about.RecipeHelp"
    android:background="#000000">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Recipe Help"
        android:id="@+id/textView"
        android:textAlignment="center"
        android:layout_marginTop="24dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:layout_below="@+id/textView"
        android:layout_marginTop="42dp"
        android:src="@drawable/up_quark"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        android:layout_below="@+id/textView"
        android:layout_marginTop="42dp"
        android:layout_marginLeft="72dp"
        android:src="@drawable/up_quark"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView4"
        android:layout_below="@+id/textView"
        android:layout_marginTop="42dp"
        android:layout_marginLeft="144dp"
        android:src="@drawable/down_quark"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView6"
        android:layout_below="@+id/textView"
        android:layout_marginTop="42dp"
        android:layout_marginLeft="246dp"
        android:src="@drawable/proton"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:layout_below="@+id/textView"
        android:layout_marginTop="152dp"
        android:src="@drawable/up_quark"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        android:layout_below="@+id/textView"
        android:layout_marginTop="152dp"
        android:layout_marginLeft="72dp"
        android:src="@drawable/down_quark"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView4"
        android:layout_below="@+id/textView"
        android:layout_marginTop="152dp"
        android:layout_marginLeft="144dp"
        android:src="@drawable/down_quark"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView6"
        android:layout_below="@+id/textView"
        android:layout_marginTop="152dp"
        android:layout_marginLeft="246dp"
        android:src="@drawable/neutron"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:layout_below="@+id/textView"
        android:layout_marginTop="262dp"
        android:src="@drawable/proton"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        android:layout_below="@+id/textView"
        android:layout_marginTop="262dp"
        android:layout_marginLeft="144dp"
        android:src="@drawable/electron"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView4"
        android:layout_below="@+id/textView"
        android:layout_marginTop="262dp"
        android:layout_marginLeft="246dp"
        android:src="@drawable/neutron"/>


</RelativeLayout>
